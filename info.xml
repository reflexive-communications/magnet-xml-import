<?xml version="1.0"?>
<extension key="magnet-xml-import" type="module">
    <file>magnet_xml_import</file>
    <name>Magnet Bank Contribution Import</name>
    <description>This extension lets you import contribution transactions from Magnet Bank XML</description>
    <license>AGPL-3.0</license>
    <authors>
        <author>
            <name>Sandor Semsey</name>
            <email>sandor@es-progress.hu</email>
            <role>Maintainer</role>
        </author>
        <author>
            <name>akosgarai</name>
            <email>szlem01@gmail.com</email>
            <role>Developer</role>
        </author>
    </authors>
    <urls>
        <url desc="Main Extension Page">https://github.com/reflexive-communications/magnet-xml-import</url>
        <url desc="Documentation">https://github.com/reflexive-communications/magnet-xml-import</url>
        <url desc="Support">https://github.com/reflexive-communications/magnet-xml-import/issues</url>
        <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
    </urls>
    <releaseDate>2023-03-16</releaseDate>
    <version>1.2.0</version>
    <develStage>stable</develStage>
    <compatibility>
        <ver>5.27</ver>
    </compatibility>
    <classloader>
        <psr0 prefix="CRM_" path=""/>
        <psr4 prefix="Civi\" path="Civi"/>
    </classloader>
    <mixins>
        <mixin>menu-xml@1.0.0</mixin>
    </mixins>
    <civix>
        <namespace>CRM/MagnetXmlImport</namespace>
        <format>22.12.1</format>
    </civix>
</extension>
